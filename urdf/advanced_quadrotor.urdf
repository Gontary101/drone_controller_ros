  <gazebo reference="flight_controller">
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <imu>
        <noise>
          <type>gaussian</type>
          <rate>
            <mean>0</mean>
            <stddev>0.0002</stddev>
          </rate>
          <accel>
            <mean>0</mean>
            <stddev>0.001</stddev>
          </accel>
        </noise>
      </imu>
    </sensor>
    <sensor name="gps_sensor" type="gps">
      <always_on>true</always_on>
      <update_rate>10</update_rate>
      <gps>
        <position_sensing>
          <horizontal>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>0.5</stddev>
            </noise>
          </horizontal>
          <vertical>
            <noise type="gaussian">
              <mean>0</mean>
              <stddev>1.0</stddev>
            </noise>
          </vertical>
        </position_sensing>
      </gps>
    </sensor>
    <sensor name="mag_sensor" type="magnetometer">
      <always_on>true</always_on>
      <update_rate>50</update_rate>
      <magnetometer>
        <noise>
          <type>gaussian</type>
          <mean>0</mean>
          <stddev>0.005</stddev>
        </noise>
      </magnetometer>
    </sensor>
  </gazebo>
  <!-- Camera Sensor on the Camera Link -->
  <gazebo reference="camera_link">
    <sensor name="camera_sensor" type="camera">
      <always_on>true</always_on>
      <update_rate>30</update_rate>
      <camera>
        <horizontal_fov>1.57</horizontal_fov>
        <image>
          <width>640</width>
          <height>480</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.1</near>
          <far>100</far>
        </clip>
        <noise>
          <type>gaussian</type>
          <mean>0</mean>
          <stddev>0.007</stddev>
        </noise>
        <distortion>
          <k1>0.1</k1>
          <k2>0.01</k2>
          <k3>0.001</k3>
          <p1>0.001</p1>
          <p2>0.001</p2>
        </distortion>
      </camera>
      <plugin filename="libgazebo_ros_camera.so" name="camera_plugin">
        <ros>
          <namespace>/camera</namespace>
          <remapping>image_raw:=image_raw</remapping>
        </ros>
      </plugin>
    </sensor>
  </gazebo>
  <!-- Lidar Sensor Plugin -->
  <gazebo reference="lidar_link">
    <sensor name="lidar_sensor" type="ray">
      <always_on>true</always_on>
      <update_rate>30</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>360</samples>
            <resolution>1</resolution>
            <min_angle>-3.14</min_angle>
            <max_angle>3.14</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="lidar_plugin">
        <ros>
          <namespace>/lidar</namespace>
          <remapping>scan:=scan</remapping>
        </ros>
      </plugin>
    </sensor>
  </gazebo>
  <!-- Additional Sensor: Downward-Facing Sonar -->
  <gazebo reference="base_link">
    <sensor name="sonar_sensor" type="ray">
      <always_on>true</always_on>
      <update_rate>20</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1</samples>
            <min_angle>0</min_angle>
            <max_angle>0</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.2</min>
          <max>5.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_range.so" name="sonar_plugin">
        <ros>
          <namespace>/sonar</namespace>
          <remapping>range:=sonar_range</remapping>
        </ros>
      </plugin>
    </sensor>
  </gazebo>
  <!-- Additional Sensor: Barometer (Pressure Sensor) -->
  <gazebo reference="base_link">
    <sensor name="barometer_sensor" type="pressure">
      <always_on>true</always_on>
      <update_rate>20</update_rate>
      <pressure>
        <noise>
          <type>gaussian</type>
          <mean>0</mean>
          <stddev>0.02</stddev>
        </noise>
      </pressure>
      <plugin filename="libgazebo_ros_pressure.so" name="barometer_plugin">
        <ros>
          <namespace>/baro</namespace>
          <remapping>pressure:=baro_pressure</remapping>
        </ros>
      </plugin>
    </sensor>
  </gazebo>
  <!-- Example: Wind and Rotor Aerodynamics Plugins on Base Link -->
  <gazebo reference="base_link">
    <plugin filename="libgazebo_wind.so" name="wind_plugin">
      <wind_velocity>0.0 0.0 0.0</wind_velocity>
      <wind_variance>0.1 0.1 0.1</wind_variance>
    </plugin>
    <plugin filename="librotor_aero_dynamics.so" name="rotor_aero_plugin">
      <!-- Example parameters; adjust to match your hardware -->
      <thrust_coefficient>0.1</thrust_coefficient>
      <drag_coefficient>0.01</drag_coefficient>
    </plugin>
  </gazebo>
</robot>
